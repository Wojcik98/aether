{
    "C_Cpp.intelliSenseEngine": "default",
    "files.associations": {
        "*.sdf": "xml",
        "*.h": "c",
    },
    "editor.formatOnSave": true,
    "ros.distro": "jazzy",
    "ros.rosSetupScript": "/opt/ros/jazzy/install/setup.bash",
    "search.exclude": {
        "ros_ws/build": true,
        "ros_ws/install": true,
        "ros_ws/log": true
    },
    "cmake.ignoreCMakeListsMissing": true,
    "cmsis-csolution.actions": [
        {
            "path": "stm/aether.csolution.yml",
            "target": "Flash",
            "config": {
                "run": "Flash Device",
                "debug": "Arm Debugger"
            }
        }
    ]
}