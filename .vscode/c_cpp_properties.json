{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "/opt/esp/tools/xtensa-esp-elf/esp-13.2.0_20230928/xtensa-esp-elf/bin/xtensa-esp32s3-elf-gcc",
            "compileCommands": "${workspaceFolder}/esp/build/compile_commands.json",
            "includePath": [
                "/opt/esp/idf/components/**",
                "${workspaceFolder}/esp/**",
                "${workspaceFolder}/aether/**"
            ],
            "browse": {
                "path": [
                    "/opt/esp/idf/components",
                    "${workspaceFolder}/esp",
                    "${workspaceFolder}/aether"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "ROS2",
            "compilerPath": "/usr/bin/gcc",
            "compileCommands": "${workspaceFolder}/ros_ws/build/compile_commands.json",
            "includePath": [
                "/opt/ros/humble/include/**",
                "${workspaceFolder}/ros_ws/**",
                "${workspaceFolder}/aether/**",
                "/usr/include/**"
            ],
            "browse": {
                "path": [
                    "/opt/ros/humble/include",
                    "${workspaceFolder}/ros_ws",
                    "${workspaceFolder}/aether",
                    "/usr/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}